<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-subcorpus"
          xml:lang="mul">
   <desc xml:lang="es">
      <term>Subcorpus</term>
   </desc>
   <desc xml:lang="en">
      <term>Subcorpora</term>
   </desc>
   <category xml:id="reference">
      <catDesc xml:lang="es">
         <term>Corpus de referencia</term>: aquel que incluye todos los textos compilados hasta el 31 de octubre de 2019.</catDesc>
      <catDesc xml:lang="en">
         <term>Reference</term>: reference subcorpus, until 2019-10-31.</catDesc>
   </category>
   <category xml:id="covid">
      <catDesc xml:lang="es">
         <term>Corpus COVID</term>: Aquel que incluye todos los textos compilados a partir del 1 de noviembre de 2019.</catDesc>
      <catDesc xml:lang="en">
         <term>COVID</term>: COVID subcorpus, from 2019-11-01 onwards</catDesc>
   </category>
</taxonomy>
