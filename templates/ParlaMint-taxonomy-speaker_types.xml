<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-speaker_types"
          xml:lang="mul">
   <desc xml:lang="es">
      <term>Tipos de oradores</term>
   </desc>
   <desc xml:lang="en">
      <term>Types of speakers</term>
   </desc>
   <category xml:id="chair">
      <catDesc xml:lang="es">
         <term>Presidencia</term>: persona que preside la sesion</catDesc>
      <catDesc xml:lang="en">
         <term>Chairman</term>: chairperson of a meeting</catDesc>
   </category>
   <category xml:id="regular">
      <catDesc xml:lang="es">
         <term>Diputado/a</term>: representante en el Congreso</catDesc>
      <catDesc xml:lang="en">
         <term>Regular</term>: a regular speaker at a meeting</catDesc>
   </category>
   <category xml:id="guest">
      <catDesc xml:lang="es">
         <term>Orador/a invitado/a</term>: persona invitada a hablar ante el Pleno</catDesc>
      <catDesc xml:lang="en">
         <term>Guest</term>: a guest speaker at a meeting</catDesc>
   </category>
</taxonomy>
